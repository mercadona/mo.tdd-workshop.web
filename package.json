{
  "name": "tdd-workshop-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:sta": "vite build --mode sta",
    "build:prod": "vite build --mode prod",
    "test": "vitest",
    "test-staging": "vitest --mode sta",
    "test:coverage": "vitest --coverage",
    "preview": "vite preview",
    "lint": "eslint --cache --ext .ts,.tsx src --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint --cache --ext .ts,.tsx src --fix",
    "format": "prettier --cache --write \"src/**/*.{ts,tsx,css,json,md}\"",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "globals": "^17.2.0",
    "jsdom": "^28.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "history": "^5.3.0",
    "normalize.css": "^8.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0"
  }
}
